pipeline{
    tools{
        jdk 'mymavenjdk4'
        maven 'mymaven4'
        }
    agent none
    stages{
        stages('checkout'){
            agent any
            steps{
                git 'https://github.com/wakaleo/game-of-life.git'
            }
        }
        stage('compile'){
            agent any
            steps{
                sh 'mvn compile'
                }
        }
       stage('test'){
            agent any
            steps{
                sh 'mvn test' 
            }
       }
       stage('package'){
            agent any
            steps{
                sh 'mvn package'
            }
       }
    }
}
